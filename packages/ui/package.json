		"ariakit": "2.0.0-next.44",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^4.1.0",
		"@react-icons/all-files": "^4.1.0",
